/**
 * Set up all the different outcomes of the cards
 */

public class CardSetup {

	private String cardType;

	private String upOrDown;
	private int cardNumber;

	/**
	 * 
	 * @param cardType
	 * @param upOrDown
	 * @param cardNumber
	 * Constructor for the CardSetup Class
	 */
	public CardSetup(String cardType, String upOrDown, int cardNumber){
		this.cardType=cardType;
		this.upOrDown=upOrDown;
		this.cardNumber=cardNumber;
	}

	/**
	 * @return the type of card
	 */
	public String getCardType(){
		return cardType;
	}

	/**
	 * 
	 * @param set the card type
	 */
	public void setCardType(String cardType){
		this.cardType=cardType;
	}

	/**
	 * @return the card number
	 */
	public int getCardNumber(){
		return cardNumber;
	}

	/**
	 * 
	 * @param cardNumber
	 * set the card number
	 */
	public void setCardNumber(int cardNumber){
		this.cardNumber=cardNumber;
	}

	/**
	 * 
	 * @return if it going up or down
	 */
	public String getUpOrDown(){
		return upOrDown;
	}

	/**
	 * 
	 * @param upOrDown 
	 * Set if it is going up or down
	 */
	public void setUpOrDown(String upOrDown){
		this.upOrDown=upOrDown;
	}



}
